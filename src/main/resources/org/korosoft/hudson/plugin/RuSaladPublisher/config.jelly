<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form">
    <f:entry title="RuSalad report folder" field="reportFolder">
        <f:textbox/>
    </f:entry>
    <f:advanced>
        <f:entry title="Number of scenarios to fail to mark build as 'FAILED'" field="buildFailThreshold">
            <f:textbox/>
